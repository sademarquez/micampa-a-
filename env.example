# Variables de entorno para el frontend (PWA)
# Copia este archivo como .env y configura tus valores

# URL del backend n8n en Cloud Run
VITE_N8N_API_URL=https://agora-backend-894766450086.us-central1.run.app

# URL de desarrollo local (opcional)
VITE_N8N_DEV_URL=http://localhost:5678

# Configuración de Gemini (si la necesitas)
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# Configuración de Supabase (si la necesitas)
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Modo de producción
VITE_PRODUCTION_MODE=true 